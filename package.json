{
  "name": "music-player",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://adamnt.github.io/music-player",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --watchAll=false",
